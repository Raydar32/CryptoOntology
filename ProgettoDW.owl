        <rdf:type rdf:resource="http://www.semanticweb.org/alessandro/ontologies/2021/4/ProgettoDW#Blockchain"/>
        <hasArchitecture rdf:resource="http://www.semanticweb.org/alessandro/ontologies/2021/4/ProgettoDW#BitcoinCore"/>
